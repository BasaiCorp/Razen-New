fun greet(name: str) {
    println(f"Hello, {name}")
}

fun add(a: int, b: int) -> int {
    return a + b
}

fun main() {
    // This should work fine
    greet("World")
    
    // This should cause a type error: passing int to str parameter
    var age = 25
    greet(age)
    
    // This should also cause a type error: passing string to int parameter
    add("hello", 5)
}
