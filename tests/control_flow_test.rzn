// ===== SIMPLE CONTROL FLOW TEST =====
// Testing basic control flow features

fun main() {
    println("=== CONTROL FLOW TEST ===")
    
    // ===== IF-ELIF-ELSE TEST =====
    println("1. If-Elif-Else Test:")
    var score = 85
    
    if score >= 90 {
        println("Grade: A")
    } elif score >= 80 {
        println("Grade: B") 
    } else {
        println("Grade: Other")
    }
    
    println("Control flow test completed")
}
