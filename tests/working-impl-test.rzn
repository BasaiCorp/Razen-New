// Working impl test - using direct method calls

struct Person {
    name: str,
    age: int
}

impl Person {
    fun hello() {
        println("Hello from Person method!")
    }
    
    fun greet_alice() {
        println("Hello Alice!")
    }
    
    fun add_numbers() -> int {
        return 5 + 3
    }
}

fun main() {
    println("=== Razen Impl System Test ===")
    
    // Direct method calls (these work now!)
    hello()
    greet_alice()
    
    var result = add_numbers()
    println("Result: " + result)
    
    println("=== Test Complete ===")
}
