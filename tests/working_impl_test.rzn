// Test impl, self, new without return types
struct Person {
    name: str,
    age: int
}

impl Person {
    // Static method without return type
    fun new(name: str, age: int) {
        println("Creating person")
    }
    
    // Instance method with self
    fun greet(self) {
        println("Hello from person")
    }
    
    // Method with self and parameters
    fun introduce(self, to: str) {
        println("Introducing to someone")
    }
}

fun main() {
    println("=== Testing impl/self/new ===")
    
    // Test struct creation
    var person1 = Person { name: "Alice", age: 25 }
    person1.greet()
    
    // Test static method call
    Person.new("Bob", 30)
    
    // Test method with parameters
    person1.introduce("Charlie")
    
    println("=== All tests completed! ===")
}
