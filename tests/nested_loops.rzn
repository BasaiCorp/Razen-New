fun main() {
    println("Testing nested loops:")
    
    for i in 1..4 {
        for j in 1..4 {
            print(f"{i}x{j}={i * j} ")
        }
        println("")
    }
    
    println("\nTesting nested loops with break:")
    var count = 0
    while count < 3 {
        println(f"Outer loop: {count}")
        for inner in 1..6 {
            if inner == 3 {
                break
            }
            println(f"  Inner: {inner}")
        }
        count = count + 1
    }
}
