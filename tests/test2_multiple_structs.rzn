struct Animal {
    name: str,
    species: str
}

impl Animal {
    fun new(name: str, species: str) {
        println("Creating new animal")
    }
    
    fun speak(self) {
        println("Animal is making a sound")
    }
    
    fun eat(self, food: str) {
        println("Animal is eating")
    }
}

struct Vehicle {
    brand: str,
    model: str
}

impl Vehicle {
    fun new(brand: str, model: str) {
        println("Creating new vehicle")
    }
    
    fun start_engine(self) {
        println("Engine started!")
    }
    
    fun drive(self, destination: str) {
        println("Driving to destination")
    }
    
    fun stop_engine(self) {
        println("Engine stopped!")
    }
}

fun main() {
    println("=== Test 2: Multiple Structs ===")
    
    // Test Animal methods
    Animal.new("Buddy", "Dog")
    var animal = Animal { name: "Cat", species: "Feline" }
    animal.speak()
    animal.eat("fish")
    
    // Test Vehicle methods  
    Vehicle.new("Toyota", "Camry")
    var car = Vehicle { brand: "Honda", model: "Civic" }
    car.start_engine()
    car.drive("office")
    car.stop_engine()
    
    println("=== Test 2 PASSED! ===")
}
