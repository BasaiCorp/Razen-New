// Comprehensive test for arrays and maps

// Arrays with different types
var numbers: [int] = [1, 2, 3, 4, 5]
var names: [str] = ["Hanuman", "Ram", "Brahma"]
var prices: [float] = [19.99, 29.99, 39.99]

// Maps with different value types
var ages: {str: int} = {"Ram": 25, "Hanuman": 30}
var scores: {str: float} = {"math": 95.5, "science": 88.0}

// Array indexing
println(numbers[0])
println(numbers[2])
println(names[1])

// Map indexing
println(ages["Ram"])
println(scores["math"])

// Nested arrays
var matrix: [[int]] = [[1, 2], [3, 4]]
println(matrix[0])
println(matrix[1])
