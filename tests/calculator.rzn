// tests/calculator.rzn - Simple calculator module for testing

pub fun Add(a: int, b: int) -> int {
    return a + b
}

pub fun Sub(a: int, b: int) -> int {
    return a - b
}

pub fun Multiply(a: int, b: int) -> int {
    return a * b
}

pub fun Divide(a: int, b: int) -> float {
    return a / b
}

pub const PI: float = 3.14159
